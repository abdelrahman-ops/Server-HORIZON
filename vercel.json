{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api-docs(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/docs",
      "dest": "/index.js",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ],
  "rewrites": [],
  "headers": [
    {
      "source": "/api-docs",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' 'unsafe-inline' cdn.jsdelivr.net; img-src 'self' data:;"
        }
      ]
    }
  ]
}